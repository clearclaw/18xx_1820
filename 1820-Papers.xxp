  [[cousin-{{ cousin_typ}}{{ sheet + 1 }}]]
    num_across_x = 2
    num_across_y = 3
    tile_x = 255
    tile_y = 236
    desc1 = "Assets & Treasury"
    desc1_font = "Times-Italic", 14
