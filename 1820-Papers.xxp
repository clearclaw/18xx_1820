    note1_colour = ${BLACK}
    token_count = 0
    token_bottom_colour = ${BLACK}
    token_bottom_stripe_colour = ${BLACK}
    token_colour = ${BLACK}
    token_top_colour = ${BLACK}
    token_top_stripe_colour = ${BLACK}
    token_name_colour = ${WHITE}
    title_colour = ${WHITE}
