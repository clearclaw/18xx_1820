    {% set index = sheet * page_size %}
    {% for tile in range (page_size) %}
      {% if index + tile >= cousin_count %}
        {% continue %}
      {% endif %}
      {% set cousin = cousins[index + tile] %}
    [[[tile_{{ tiles[tile] }}]]]
      cost = "Cost: ${{ cousin.cost }}"
      revenue = "Revenue: ${{ cousin.revenue }}"
